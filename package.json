		"clean": "gulp clean",
		"cleanall": "gulp cleanall",
		"lint": "gulp lint",
		"test": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=\"./test/tsconfig.json\" mocha",
		"format": "gulp format",
		"release": "shipjs prepare"
	},
