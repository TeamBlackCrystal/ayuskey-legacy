		"start": "node ./index.js",
		"init": "yarn migrate",
		"ormconfig": "node ./built/ormconfig.js",
		"migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
		"migrateandstart": "yarn migrate && yarn start",
		"build": "yarn build-webpack && yarn build-ts && yarn build-gulp",
		"build:product": "cross-env NODE_ENV=production yarn build",
		"build-webpack": "webpack",
		"build-ts": "tsc -p src/tsconfig.json || echo done. && tsc-alias -p src/tsconfig.json",
		"build-gulp": "gulp build",
		"webpack": "webpack",
		"dev": "echo \"**コミットをしたら再起動が必要!!/Need to restart after a commit!!**\" && run-p watch-webpack gulpstart",
		"gulpstart": "echo \"**コミットをしたら再起動が必要!!/Need to restart after a commit!!**\" && run-s gulp start",
		"watch-webpack": "echo \"**コミットをしたら再起動が必要!!/Need to restart after a commit!!**\" && webpack --watch",
		"gulp": "gulp build",
		"clean": "gulp clean",
		"cleanall": "gulp cleanall",
		"lint": "gulp lint",
		"test": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=\"./test/tsconfig.json\" mocha",
		"format": "gulp format",
		"release": "shipjs prepare"
	},
